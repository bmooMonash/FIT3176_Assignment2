{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "config": {"view": {"stroke": ""}},
    "width": 500,
    "height": 50,
    "data": { "url": "https://raw.githubusercontent.com/bmooMonash/FIT3176_Assignment2/main/shark%20attacks.csv"},
    "transform": [
      {"filter":{"field":"Sex", "oneOf":["F", "M"]}},
      {"filter": {"field": "Country", "oneOf": ["USA", "AUSTRALIA", "SOUTH AFRICA", "BAHAMAS", "NEW ZEALAND"]}},
      {"window": [{"op": "rank", "as": "rank"}], "groupby": ["Sex", "Type"]},
      {"filter":{"field":"rank", "oneOf":[1, 50, 100, 150, 200, 250 ,300, 350, 400, 450, 500, 550, 600, 650, 700, 750, 800]}}
    ],
    "mark":{"type": "image", "baseline":"middle"},
    "encoding": {
      "url": {
              "condition": {
              "test": "datum['Sex'] == 'M'", 
              "value": "https://raw.githubusercontent.com/bmooMonash/FIT3176_Assignment2/main/Shark.png"},
              "value": "https://raw.githubusercontent.com/bmooMonash/FIT3176_Assignment2/main/Shark2.png"},
      "x": {"field": "rank", "type": "ordinal", "axis": null},
      "y": {"field": "Sex", "type": "nominal", "axis": null, "sort":{"field":"rank"}},
      "row": {"field": "Type", "spacing": 30}
      }
  }